<div id="<%= dom_id contact %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Job:</strong>
    <%= contact.job_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date:</strong>
    <%= contact.date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Contact type:</strong>
    <%= contact.contact_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Person:</strong>
    <%= contact.person %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Notes:</strong>
    <%= contact.notes %>
  </div>
</div>
